   "source": [
    "from micrograd.engine import Value\n",
    "from micrograd.plot import draw_dot\n",
    "\n",
    "a = Value(-2.0, label='a')\n",
    "b = Value(3.0, label='b')\n",
    "d = a*b; d.label = 'd' \n",
    "e = a+b; e.label = 'e' \n",
    "f = d*e; f.label = 'f' \n",
    "\n",
    "f.backward()\n",
    "\n",
    "draw_dot(f)"
   ]
  }
